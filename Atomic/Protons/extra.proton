

# CSharp
set random() {
  return EXCSFunc('Console.WriteLine("random"); return new NullVal();', null);
}


set question(message) {
  set results >> prompt(message + " Y/n ");

  results >> toLower(results);
  if results = 'y': {
    return true;
  }
  else if results = 'n': {
    return false;
  }
  else {
    return null;
  }
}
